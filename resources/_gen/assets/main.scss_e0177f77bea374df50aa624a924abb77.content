@import url("https://fonts.googleapis.com/css2?family=JetBrains+Mono:ital,wght@0,100..800;1,100..800&family=Open+Sans:ital,wght@0,300..800;1,300..800&display=swap");body{font-family:"Open Sans", sans-serif;font-size:90%}html,body{color:black;background:#f7f7f2;line-height:1.5;margin:0;min-height:100%;overflow-wrap:break-word;text-align:justify;hyphens:auto;word-break:break-word}body li{margin:0.4rem 0}*:target{background:yellow}.w{max-width:800px;margin:0 auto;padding:1rem}hr{margin:2rem 0;text-align:center;border:0}hr:before{content:"--- END"}hr:after{content:attr(data-content) " ---"}.hglt a,li a,.content a{text-decoration:none;position:relative;color:black;text-decoration:none}.hglt a::after,li a::after,.content a::after{content:'';position:absolute;width:0;height:2px;display:block;margin-top:1px;left:0;background:black;opacity:40%;transition:width 0.2s ease}.hglt a:hover::after,li a:hover::after,.content a:hover::after{width:100%}ul{padding-left:5px;list-style:circle;margin-left:25px;position:relative}@media (max-width: 768px){body{font-size:90%}table tr{font-size:80%}ul{margin-left:15px}.w{padding:0.5rem}blockquote{padding:0.8rem}img{margin-bottom:1rem}.dsclmr{font-size:1em}}header{display:flex;align-items:center}header .house{margin-right:5px}header .house i{font-size:28px}header h1{margin:0;font-size:28px}h1{font-size:2em}h2,h3,h4,h5,h6{margin-top:1.2em}.house{font-size:2em;display:flex}header a{text-decoration:none;color:black}.footer a{color:black;text-decoration:none}table{width:100%;border:2px solid black;border-collapse:collapse}tr{width:100%;border:1px solid black;border-collapse:collapse}.skills{font-family:'JetBrains Mono', monospace}.key{font-weight:bold;text-align:center}pre{margin:10px;padding:10px 15px;border-radius:5px;border:2px solid black;overflow-x:auto;box-shadow:0px 0px 10px #c7c7a2}code{font-family:'JetBrains Mono', monospace;font-size:90%}code.has-jax{-webkit-font-smoothing:antialiased;background:inherit !important;border:none !important;font-size:100%}blockquote{font-family:'JetBrains Mono', monospace;font-size:10pt;border:solid #444;padding:1.5rem;margin:10px 0;border-radius:5px}blockquote p{margin:0}img{max-width:100%;align-content:center;border-radius:5px;margin:0 auto;box-shadow:0px 0px 10px #c7c7a2}figcaption{text-align:center;opacity:0.5}.dsclmr{padding:10px;font-weight:bold;color:#fcf8f0;background:#cc6666;border-radius:5px;text-align:center;text-transform:uppercase}.webring{display:flex;flex-wrap:wrap;justify-content:space-around;gap:1rem}@media (max-width: 768px){.webring{flex-direction:row;align-items:center}}
